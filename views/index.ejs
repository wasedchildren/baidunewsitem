    <script src="http://cdn.bootcss.com/jquery/2.2.4/jquery.min.js"></script>
    <script src="/javascripts/index.js"></script>

</head>

<body>
    <header class="main-header">
        <nav class="topbar container">
            <nav class="row">

                <div class="col-md-12 border side">





                </div>

            </nav>


            <ul class="menu">
                <li role="presentation">
                    <a class="style-li" href="javascript:void(0)">精选</a></li>
                <li role="presentation"><a href="javascript:void(0)">百家</a></li>
                <li role="presentation"><a href="javascript:void(0)">本地</a></li>
